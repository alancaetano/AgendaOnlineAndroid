<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="agendaonline.agendaonlineapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PrincipalActivity"
            android:label="@string/title_activity_principal" >
        </activity>
        <activity
            android:name=".InformacoesAdicionaisActivity"
            android:label="@string/title_activity_informacoes_adicionais" >
        </activity>
        <activity
            android:name=".RecadoActivity"
            android:label="@string/title_activity_recado" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="agendaonline.agendaonlineapp.PrincipalActivity" />
        </activity>
        <activity
            android:name=".ChatActivity"
            android:label="@string/title_activity_chat" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="agendaonline.agendaonlineapp.PrincipalActivity" />
        </activity>
        <activity
            android:name=".SelecionarContatoActivity"
            android:label="@string/title_activity_selecionar_contato" >
        </activity>
    </application>

</manifest>
